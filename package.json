  "license": "MIT",
  "devDependencies": {
    "cypress": "^8.2.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-iframe": "^1.0.1",
    "cypress-mailosaur": "^2.3.3",
    "eslint": "^7.32.0",
    "eslint-plugin-cypress": "^2.13.2",
    "faker": "^5.5.3"
  }
}
